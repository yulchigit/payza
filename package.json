    "api:install": "npm --prefix backend install",
    "api:migrate": "npm --prefix backend run migrate",
    "api:dev": "npm --prefix backend run dev",
    "api:start": "npm --prefix backend run start",
    "release:bootstrap:prod-env": "node scripts/bootstrap-prod-env.js",
    "release:secret": "node scripts/generate-jwt-secret.js",
    "release:preflight": "node scripts/release-preflight.js",
    "release:preflight:prod": "node scripts/release-preflight.js --target=production",
    "release:verify": "npm run release:preflight && npm --prefix backend test && npm run build",
    "release:verify:prod": "npm run release:preflight:prod && npm --prefix backend test && npm run build",
    "release:android": "npm run release:verify && npm run mobile:sync",
    "mobile:sync": "npm run build && npx cap sync",
    "mobile:android": "npm run mobile:sync && npx cap open android",
    "mobile:ios": "npm run mobile:sync && npx cap open ios",
    "mobile:bundle:release": "cd android && gradlew.bat bundleRelease"
  },
